{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es2015", "dom"],
    
    // "allowUnreachableCode": false,
    // "allowUnusedLabels": false,
    "removeComments": false,
    "preserveValueImports": false,
    "verbatimModuleSyntax": false,

    "baseUrl": ".",
    "paths": {
      "@lib/*": ["./lib/*"]
    },
  },
  "include": ["lib"],
}
